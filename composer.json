{
    "autoload": {
        "psr-4": {
            "Kani\\Nemesis\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "description": "Nemesis: CORS par token et quota par domaine pour Laravel.",
    "extra": {
        "laravel": {
            "providers": [
                "Kani\\Nemesis\\NemesisServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "name": "kani/laravel-nemesis",
    "require": {
        "laravel/framework": "^12.0",
        "php": ">=8.1"
    },
    "scripts": {
        "pre-package-uninstall": [
            "bash scripts/uninstall.sh"
        ]
    },
    "type": "library",
    "version": "0.2.5"
}
{
    "autoload": {
        "psr-4": {
            "Kani\\Nemesis\\": "src/"
        }
    },
    "description": "Nemesis: CORS par token et quota par domaine pour Laravel.",
    "extra": {
        "laravel": {
            "providers": [
                "Kani\\Nemesis\\NemesisServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "name": "kani/laravel-nemesis",
    "require": {
        "laravel/framework": "^12.0",
        "php": ">=8.1"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"if (file_exists('vendor/autoload.php')) { require_once 'vendor/autoload.php'; if (class_exists('Kani\\\\Nemesis\\\\NemesisServiceProvider')) { Kani\\\\Nemesis\\\\NemesisServiceProvider::cleanup(); } }\""
        ]
    },
    "type": "library",
    "version": "0.o.2"
}
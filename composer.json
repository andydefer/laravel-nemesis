{
    "autoload": {
        "psr-4": {
            "Kani\\Nemesis\\": "src/"
        }
    },
    "description": "Nemesis: CORS par token et quota par domaine pour Laravel.",
    "extra": {
        "laravel": {
            "providers": [
                "Kani\\Nemesis\\NemesisServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "name": "kani/laravel-nemesis",
    "require": {
        "laravel/framework": "^12.0",
        "php": ">=8.1"
    },
    "scripts": {
        "pre-package-uninstall": [
            "@php -r \"\\$configPath = 'config/app.php'; if (file_exists(\\$configPath)) { \\$content = file_get_contents(\\$configPath); \\$pattern = '/\\\\\\\\n\\\\s*\\\\\\\\Kani\\\\\\\\\\\\\\\\Nemesis\\\\\\\\\\\\\\\\NemesisServiceProvider::class,/'; \\$newContent = preg_replace(\\$pattern, '', \\$content); if (\\$newContent !== \\$content) { file_put_contents(\\$configPath, \\$newContent); } }\"",
            "@php -r \"\\$cacheFiles = ['bootstrap/cache/packages.php', 'bootstrap/cache/services.php', 'bootstrap/cache/config.php']; foreach (\\$cacheFiles as \\$file) { if (file_exists(\\$file)) { @unlink(\\$file); } }\""
        ]
    },
    "type": "library",
    "version": "0.1.1"
}